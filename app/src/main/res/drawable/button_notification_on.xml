<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="45dp"
        android:height="45dp">
        <shape android:shape="oval">
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item
        android:bottom="0.5dp"
        android:end="0.5dp"
        android:start="0.5dp"
        android:top="0.5dp">
        <shape android:shape="oval">
            <solid android:color="@color/green_regular" />
        </shape>
    </item>
    <item
        android:bottom="5.5dp"
        android:end="5.5dp"
        android:start="5.5dp"
        android:top="5.5dp">
        <shape android:shape="oval">
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:end="6dp"
        android:start="6dp"
        android:top="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>
