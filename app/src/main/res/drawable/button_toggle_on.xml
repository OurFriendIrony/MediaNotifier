<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/transparent_100" />
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="7dp"
        android:top="7dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/black" />
            <corners android:radius="6dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="8dp"
        android:top="8dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_08" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="7dp"
        android:top="7dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_08" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="6dp"
        android:top="6dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_02" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</layer-list>
