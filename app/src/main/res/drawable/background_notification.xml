<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="45dp"
        android:height="45dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="oval">
            <solid android:color="@color/cyan_regular"/>
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:end="5dp"
        android:start="5dp"
        android:top="5dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="oval">
            <solid android:color="@color/grey_01"/>
        </shape>
    </item>
</layer-list>
